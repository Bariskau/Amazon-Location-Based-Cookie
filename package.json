    "jest": "~28.1",
    "nodemon": "^2.0.16",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "ts-node": "^10.8.1",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
